@import './common.wxss';
.defaultBtn{height: 90rpx;margin: 20rpx 30rpx 0 30rpx;background: #fff;display: flex;justify-content: space-between;align-items: center;font-size: 30rpx;color: #333;padding-left: 30rpx;box-sizing: border-box;padding-right: 38rpx;}
.explain{font-size:24rpx;display:flex;justify-content: space-between;color:#888;margin-top:30rpx;padding: 0 30rpx;}
.explain >view:nth-of-type(2){color: #0097ff;}
.ic_gantanhao{background:url(data:image/png;base64,) no-repeat;background-size:cover;width:24rpx;height:24rpx;display:inline-block;vertical-align:middle;margin-right: 10rpx;margin-top: -4rpx;}

.status .icon-selected{width: 100%;background-size: 46rpx 46rpx;background-position: center center;}
.addinfo{box-sizing:border-box;padding:20rpx 30rpx;color:#888;font-size:22rpx}
.addinfo .item{display:block;margin-top:14rpx}
.addinfo .item:first-child{margin-top:0}
.info{text-align:center;margin-top:24rpx;font-size:24rpx;color:#b2b2b2;margin-bottom: 40rpx;color: #666666;}
.info navigator{color:#6b98ff;display:inline-block}
.orange{color: #fd711c;}

.icon-selected{height: 46rpx;width: 46rpx;background-image: url(data:image/png;base64,);background-size: cover;background-repeat: no-repeat;}

.icon-noselect{background-image: url(data:image/png;base64,);}