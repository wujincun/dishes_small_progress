.pink{color:#f76260}
.blue{color:#6b98ff}
.record-item{margin-top:20rpx;background:#fff;display:flex;flex-direction:column;justify-content:space-around;min-height:160rpx;font-size:28rpx;color:#888;box-sizing: border-box;padding: 24rpx 0 24rpx 0;}
.record-item>view{display:flex;justify-content:space-between}
.record-item>view:first-child text:first-child{color:#272727}
.record-item view:last-of-type{margin-top: 26rpx;}
.summary{background:#fff;display:flex;justify-content:space-between;box-sizing:border-box;border-top:1px solid #e5e5e5;padding:24rpx 0 20rpx 0}
.summary>view{flex:1;display:flex;flex-direction:column;align-items:center;font-size:26rpx;color:#272727}
.summary>view text{font-size:22rpx;color:#888}
.summary>view .num{font-size:36rpx;margin-top:10rpx}
.income{border-right:1px solid #e5e5e5;box-sizing:border-box}
.left-text{margin-left: 25rpx;max-width: 550rpx;line-height: 36rpx;}
.right-text{margin-right: 25rpx;line-height: 36rpx;}
.income .num{color:#f76260}
.expend .num{color:#6b98ff}
.norecord-bg{box-sizing:border-box;padding-top:354rpx;min-height:400rpx;width:100%;background:url(data:image/png;base64,) no-repeat;background-position-x:50%;background-position-y:115rpx;background-size:210rpx 180rpx;font-size:30rpx;color:#888;text-align:center}