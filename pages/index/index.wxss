@import "../template/template.wxss";
.index-wrapper{width:100%}
.shop-pic{width:100%;height:260rpx}
.shop-info-wrapper{background:#fff;position:relative;padding:0 25rpx;font-size:0;margin-top: -12rpx;padding-top: 2rpx;}
.shop-logo{height:164rpx;width:164rpx;position:absolute;border:1rpx solid #fff;background:#fff;padding:2px;border-radius:8rpx;top:-36rpx}
.shop-logo image{width:100%;height:100%;border-radius: 8rpx;}
.shop-info{margin-left:189rpx;height:110rpx}
.shop-name{font-family:"Microsoft Yahei";font-size:17px;color:#000;height:24px;line-height:24px;margin-bottom:20rpx;margin-top: 22rpx;}
.shop-name .left{float: left;max-width: 88%;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;}
.shop-name .right{float: left;width: 10%;text-align: right;}
.flex-box{display:flex;justify-content:space-between}
.shop-work-time{color:#888;font-size:11px;line-height:15px}
.shop-average-cost{color:#888;font-size:11px;line-height:15px;margin-right:30rpx}
.shop-address{margin-top:32rpx;width:100%;height:75rpx;display:flex;box-sizing:border-box;}
.shop-address .address{margin-left:10rpx;height:44rpx;box-sizing:border-box;white-space:nowrap;text-overflow:ellipsis;font-size:30rpx;line-height:44rpx;vertical-align:middle;padding-right:44rpx;overflow:hidden;display:inline-block;flex:1}
.shop-support-list{padding-top:25rpx;width:100%;padding-bottom:35rpx;border-top: 1rpx solid #e5e5e5;}
.support-item{display:inline-block;vertical-align:top;box-sizing:border-box;padding:0 20rpx;border:0.5px solid #ccc;font-size:22rpx;text-align:center;color:#888;margin-right:40rpx;border-radius:4px;height:48rpx;line-height:48rpx}
.gary-bg{width:100%;height:16rpx;background:#f5f5f5}
.operations{background:#fff;margin-top:16rpx;display:flex;padding:40rpx 11rpx 34rpx 11rpx;justify-content:space-between}
.operation{width:365rpx;height:196rpx}
.operation image{display:block;width:100%;height:100%}
.nav-list{padding-top:32rpx;padding-bottom:28rpx;width:100%;background:#fff;margin-top:16rpx;display:flex;justify-content:space-around}
.nav-item{width:100rpx;height:140rpx}
.nav-item-disable{opacity: .4}
.nav-item image{display:block;width:100rpx;height:100rpx}
.nav-item .text{font-size:28rpx;line-height:40rpx;height:40rpx;width:100%;text-align:center}
.tel-wrapper{position:fixed;top:0;bottom:0;right:0;left:0}
.tel-wrapper .mask{width:100%;height:100%;background:#000;opacity:.5;-ms-filter:blur(5px);filter:blur(5px)}
.tel-content{background:#fff;position:absolute;bottom:0;width:100%}
.tel-content view{height:100rpx;font-size:36rpx;line-height:100rpx;text-align:center}
.tel-call{color:#0ab70e}
.tel-cannel{box-sizing:border-box;border-top:12rpx solid rgba(205,205,209,.3)}
.clear{zoom:1}
.clear::after{content:"";display:block;overflow:hidden;clear:both;width:100%;height:100%}
.icon-arrow-right{height:24px;line-height:24px;background:url(data:image/png;base64,) no-repeat;background-size:contain;vertical-align:middle;margin-top: -2px;}
.icon-tel{flex:0 0 44rpx;margin-left:24rpx;background:url(data:image/png;base64,);background-size:contain;vertical-align:middle}
.icon-location{background:url(data:image/png;base64,) no-repeat;background-size:contain;vertical-align:middle;flex:0 0 44rpx}
