page{min-height: 100%;background:#fff;border-top: 2rpx solid rgba(0,0,0,.05);}
.choose-shop-wrapper{width:100%;height:100%;background:#fff}
.choose-item{height:124rpx;background:#fff;display:flex;justify-content:space-between;padding-left:60rpx;padding-right:40rpx;align-items:center;color:#000;box-sizing: border-box;width: 100%;border-bottom:1rpx solid rgba(0,0,0,.05); }
.choose-item-active{background:#f2f2f2;color:#f56d00;border: none;}
.item-name{font-size:30rpx;line-height:42rpx}
.choose-item .icon-selected{background:url(data:image/png;base64,) no-repeat;background-size:contain}
.choose-item-active .icon-selected{background:url(data:image/png;base64,) no-repeat;background-size:contain}
