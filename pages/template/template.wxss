.cont {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #f5f5f5;
  text-align: center;
  z-index: 999;
}

.cont-pic {
  width: 214rpx;
  height: 204rpx;
  margin: 170rpx auto 50rpx auto;
}

.cont-pic image {
  width: 100%;
  height: 100%;
}

.cont-text {
  margin-bottom: 50rpx;
  font-size: 30rpx;
  color: #888888;
}

button {
  width: 370rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  background-color: #fd711c;
  border-radius: 10rpx;
}

button[disabled] {
  content: none;
}
