.swiper-content{overflow-y: auto;}
.swiper-tab{width: 100%; height: 94rpx; background: #fff; font-size: 32rpx;}
.swiper-tab-item{width: 25%; height: 100%; line-height: 94rpx; text-align: center; float: left; color: #5f5b55; position: relative;}
.swiper-tab .active>view{width: 100%; height: 6rpx; position: absolute; left: 0; bottom: 0; background: #fd711c;}
.swiper-content-item {background: #fff; margin-top: 20rpx;}
.swiper-item-top{height: 98rpx; line-height: 98rpx; padding: 0 25rpx;}
.swiper-item-middle{height: 183rpx; background: #fafafa; padding: 0 25rpx; position: relative;}
.swiper-item-bottom{height: 103rpx; line-height: 103rpx; padding: 0 25rpx; background: #fff;}
.swiper-item-top text:first-child{font-size: 32rpx; color: #272727; display: inline-block; width: 360rpx; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;}
.swiper-item-top text:last-child{text-align: right; font-size: 28rpx; color: #5f5b55; float: right;}
.swiper-item-left, .swiper-item-center, .swiper-item-right{float: left;}
.swiper-item-right{float: right; height: 100%;}
.swiper-item-left image{width: 140rpx; height: 140rpx; margin-top: 22rpx; border-radius: 0.1rem;}
.swiper-item-left>view{width: 140rpx; height: 140rpx; background: #fff; position: absolute; opacity: 0.8; top: 22rpx; display: flex; align-items: center; justify-content: center; text-align: center; flex-direction: column;}
.swiper-item-left>view text:first-child{color: #000; font-size: 28rpx; display: flex; align-items: center; height: 40rpx;}
.swiper-item-left>view text:last-child{color: #FD711C; font-size: 40rpx; display: flex; align-items: center; height: 56rpx;}
.swiper-item-center{margin-left: 19rpx;}
.swiper-item-center .table-name{height: 40rpx; line-height: 40rpx; margin-top: 17rpx; font-size: 28rpx; max-width: 510rpx; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.swiper-item-center .dish-name{height: 40rpx; line-height: 40rpx; font-size: 28rpx; color: #888; margin-top: 5rpx; max-width: 410rpx; display: flex; justify-content: space-between;}
.swiper-item-center .dish-name-pre{margin-top: 17rpx;}
/*margin-right: 26rpx;*/
.swiper-item-center .dish-name text:first-child{width: 220rpx; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.swiper-item-center .dish-name-pre text:first-child{color: #272727; display: flex; width: 220rpx; display:inline-block; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.swiper-item-center .pay-method{height: 45rpx; line-height: 45rpx; margin-top: 22rpx;}
.swiper-item-center .pay-method text:first-child{color: #888; font-size: 28rpx; display: inline-block; margin-right: 26rpx; padding: 3rpx 0; box-sizing: border-box;}
.swiper-item-center .pay-method text:last-child{color: #888; font-size: 26rpx; display: inline-block; padding: 4.5rpx 0; box-sizing: border-box; position: relative; top: -2rpx;}
.swiper-item-center .reserve-time{margin-top: 16rpx; font-size: 26rpx; color: #888888;}
.reserve .pay-method{margin-top: 10rpx; height: auto; line-height: normal;}
.swiper-item-right image:first-child{width: 110rpx; height: 35rpx; position: absolute; top: 21rpx; right: 25rpx;}
.swiper-item-right image:last-child{width: 29rpx; height: 29rpx; margin-top: 101rpx;}
.swiper-item-bottom .pay-time{width: 100%; height: 100%;}
.swiper-item-bottom .pay-time .surplus-time{font-size: 30rpx; float: left;}
.swiper-item-bottom .pay-time text{font-size: 30rpx;}
.swiper-item-bottom .pay-time .surplus-time-text{margin-top: 2rpx;}
.swiper-item-bottom .pay-time button{width: 170rpx; height: 70rpx; line-height: 70rpx; border: none; background: #fd711c; color: #fff; font-size: 28rpx; float: right; margin-top: 16rpx;}
.swiper-item-bottom-nor {height: 21rpx; background: #fff;}
.swiper-content-text {padding-top: 300rpx; color: #333; font-size: 28rpx; text-align: center; color: #666;}
.swiper-content-text image{width: 214rpx; height: 190rpx; margin-bottom: 20rpx;}
.order-nor{text-align: center; margin-top: 20rpx; font-size: 24rpx; color: #666;}


