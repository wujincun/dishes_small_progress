.order-detail-wrapper{box-sizing:border-box; padding-bottom:22rpx;width:100%;}
.order-type{display:flex;height:340rpx;flex-direction:column;justify-content:center;align-items:center;width:100%;background:#fff;border-top:1px solid #e4e4e4;box-sizing:border-box}
.order-type image{width:111rpx;height:111rpx}
.order-text{margin-top:60rpx;font-size:32rpx;color:#272727}
.location{margin-top:20rpx;background:#fff}
.location>view{padding-top:25rpx;padding-bottom:30rpx;box-sizing:border-box;font-size:30rpx;color:#272727;padding-left:60rpx;background-size:25rpx 25rpx;background-position:24rpx 50%}
.location>view:last-child{border-bottom:none}
.location text{color:#888}
.dilivery-location{margin-top:20rpx;background:#fff}
.dilivery-location>view{display:flex;align-items:center;padding-top:25rpx;padding-right:25rpx;padding-bottom:30rpx;box-sizing:border-box;font-size:30rpx;color:#272727}
.dilivery-location>view:last-child{border-bottom:none}
.dilivery-location .title{align-self:flex-start;color:#272727;text-align-last:justify;box-sizing:border-box;padding-right:24rpx;padding-left:60rpx;box-sizing:border-box}
.number{margin-left:12rpx}
.dilivery-location .cont{flex:1;color:#888}
.goods-detail{margin-top:20rpx;background:#fff}
.goods-list{padding:0 25rpx}
.goods-item{height:148rpx;box-sizing:border-box;display:flex;flex-direction:column;justify-content:center}
.goods-item:last-child{border-bottom:none}
.goods-name{display:flex;align-items:center;color:#272727;font-size:30rpx; justify-content: space-between;}
.goods-name image{width: 40rpx; height: 40rpx; margin-left: 10rpx;}
.goods-name .icon-image {width: 68rpx; height: 40rpx;}
.goods-desc{height: 40rpx; line-height: 40rpx; margin-top:10rpx;font-size:28rpx;color:#888;display:flex;justify-content:space-between}
.goods-desc text:nth-child(1) {flex: 2;}
.goods-desc text:nth-child(2) {flex: 1;}
.goods-desc text:nth-child(3) {flex: 1; text-align: right;}
.total-wrapper{border-top:1px dashed #e4e4e4;box-sizing:border-box;padding:0 24rpx;}
.total-wrapper>view{margin-top:20rpx;display:flex;justify-content:space-between;font-size:28rpx;align-items:center}
.info{margin-top:10rpx!important;color:#f49130!important;font-size:24rpx!important;display:flex;align-items:center!important;justify-content:flex-start!important;padding-bottom:26rpx;}
.add{height:84rpx; margin-top: 0 !important;}
.order-time{margin-top:20rpx;background:#fff;padding-left:25rpx;font-size:30rpx;color:#888;height:142rpx;display:flex;flex-direction:column;justify-content:center}
.uuid{margin-top:10rpx}
.order-time text{display:inline-block;width:160rpx}
.order-time>view{display: flex; justify-content: flex-start;}
.order-time view:last-child text{word-spacing:8rpx}
.order-time>view text{display: flex; justify-content: space-between;}
.time-space{width: 130rpx; display: flex; justify-content: space-between; margin-right: 14rpx;}
.del-order{margin-top:20rpx}
.del-order button{border-radius:0;font-size:30rpx;background:#fff;height:98rpx;line-height:98rpx;color:#888}
.del-order button::after{content:none}
.icon-info{background:url(data:image/png;base64,) no-repeat;width:22rpx;height:22rpx;background-size:cover;margin-right:8rpx}
.icon-tejia{margin-left:10rpx;background:url(data:image/png;base64,) no-repeat;height:28rpx;width:48rpx;background-size:contain;vertical-align: middle;}
.dilivery-location .location-info>view:first-of-type{background-size:25rpx 25rpx;background-position:24rpx 50%;background-repeat:no-repeat;background-image:url(data:image/png;base64,)}
.dilivery-time view:first-of-type{background-image:url(data:image/png;base64,);background-size:25rpx 25rpx;background-position:24rpx 50%;background-repeat:no-repeat}
.dilivery-remarks view:first-of-type{background-size:25rpx 25rpx;background-position:24rpx 50%;background-repeat:no-repeat;background-image:url(data:image/png;base64,)}
.people-num{background:url(data:image/png;base64,) no-repeat}
.table-num{background:url(data:image/png;base64,) no-repeat}
.remarks{background:url(data:image/png;base64,) no-repeat}
.timer{font-size: 32rpx; color: #6b98ff; margin-top: 17rpx;}
.footer-bottom{width: 100%; height: 100rpx; position: fixed; left: 0; bottom: 0; background: #fff;}
.footer-bottom .pay{width: 100%; height: 100%; display: flex; justify-content: center; align-items: center;}
.pay-item{flex: 1; height: 100%; box-sizing: border-box; display: flex; align-items: center; justify-content: center;}
.pay-item:first-child{border-right: 1px solid #e4e4e4; flex-direction: column;}
.pay-item:nth-child(2){font-size: 34rpx; color: #888;}
.pay-item:nth-child(3){font-size: 34rpx; color: #fff; background: #fd711c;}
.pay-item:nth-child(1) text{display: block;}
.pay-item:nth-child(1) text:first-child{font-size: 24rpx;}
.pay-item:nth-child(1) text:last-child{font-size: 38rpx; color: #e64340;}
.dilivery-remarks .title text {opacity: 0;}
/*.wx-modal-dialog{width: 560rpx; height: 291rpx; text-align: center;}*/
.common-box>view{display: flex; justify-content: space-between;}
.price-child{margin-left: 28rpx;}
.address{margin-top: 10rpx;}
.common-price-child {padding:0 0 0 20rpx; color:#666; margin-top:10rpx !important; font-size:25rpx !important;}
.pay-method{padding: 0 0 10rpx 20rpx; color: #666; margin-top: 0 !important; font-size: 25rpx !important;}
.dilivery-price .num, .total .num, .super-price .num {color: #888;}
/*.pay-method*/