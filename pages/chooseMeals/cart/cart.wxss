.clearup{background-image:url(data:image/png;base64,)}
.ic_tejia{display:inline-block;width:48rpx;height:28rpx;background:url(data:image/png;base64,);background-size:cover;vertical-align:middle}
button[disabled]{background:#fd711c;color:#fff;opacity: .4;margin-top: -1rpx;}
button::after{content: none;}
button{padding-left: 0;padding-right: 0;}
.num-title{display: inline-block;}
.footer .num .orange{display: inline-block;}


.item{height: 150rpx;box-sizing: border-box;}
 .avatar .name{height: 33rpx;line-height: 33rpx;font-size: 22rpx;}
 .goods-title{display: inline-block;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;vertical-align: middle;max-width: 240rpx;}
 .ic_tejia{margin-left: 12rpx;}

 /*  30 line: height:100%;line-height:108rpx;*/

 .icon-box{width: 40rpx;height: 40rpx;display: inline-block;vertical-align: middle;margin-left: 8rpx;}
 .icon-box image{width: 100%;height: 100%;}
 .icon-box-pack{width: 60rpx;}