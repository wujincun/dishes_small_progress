page{position:relative;box-sizing:border-box;padding-bottom:100rpx;min-height:100%}
.wrapper{min-height: 100%;padding-bottom: 20rpx;}
.tips{height:70rpx;background:#fff8e9;color:#666;text-align:center;font-size:24rpx;line-height:70rpx;display: flex;align-items: center;justify-content: center;}
.list-header{display:flex;box-sizing:border-box;padding:0 40rpx 0 30rpx;height:70rpx;border-left:8rpx solid #fd711c;color:#666;font-size:28rpx;line-height:70rpx;justify-content:space-between}
.icons{display:flex;padding:15rpx 0;width:252rpx;justify-content:space-between}
.add,.clearup{box-sizing:border-box;background-size:24rpx 24rpx;background-repeat:no-repeat;text-align:right;line-height:46rpx;flex:1;background-position-y:50%;background-position-x:40rpx}
.add{position: relative;}
.add::after{content: ' ';height: 30rpx;position: absolute;width: 1rpx;left: 144rpx;top:8rpx;background: #cccccc;}
.item{display:flex;box-sizing:border-box;padding:9rpx 40rpx 0rpx 32rpx;height:140rpx;background:#fff;align-items:center}
.item:last-child{border-bottom:none}
.avatar{flex:0 0 88rpx;height: 100%;width: 106rpx;overflow: hidden;}
.avatar image{width:88rpx;height:88rpx;border-radius: 88rpx;}
.avatar .name{margin-top:-4rpx;color:#999;text-align:center;font-size:20rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.goods-name{margin-left:32rpx;flex:1;margin-top: -4rpx;}
.list-header .title{padding: 15rpx 0;}
.title{color:#333;font-size:30rpx;height: 42rpx;line-height: 42rpx;}
.desc{font-size: 20rpx;color: #999;margin-top: 2rpx;height: 28rpx;line-height: 28rpx;}
.price{margin-top:8rpx;color:grey;font-size:28rpx}
.little{font-size:22rpx}
.orange{color:#fd711c;margin-left: 6rpx;font-size: 32rpx;}
.delline{text-decoration:line-through;margin-left: 6rpx;}
.operate{display:flex;flex:0 0 152rpx;align-items: center;}
.operate .num{color:#f63;text-align:center;flex:1;}
.btn-plus,.btn-reduce{height:46rpx;flex:0 0 46rpx}
.btn-plus image,.btn-reduce image{width:100%;height:100%}
.location{box-sizing:border-box;padding:0 30rpx;height:70rpx;border-left:8rpx solid #fd711c;color:#666;font-size:28rpx;line-height:70rpx; width: 100%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.location{box-sizing:border-box;padding-left:30rpx;height:70rpx;border-left:8rpx solid #fd711c;color:#666;font-size:28rpx;line-height:70rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.people{display:flex;padding:0 32rpx;height:108rpx;background:#fff;line-height:108rpx;font-size: 28rpx;margin-bottom: 20rpx;align-items: center;}
.people label{flex: 0 0 140rpx;}
.people input{box-sizing:border-box;padding-left:12rpx;font-size: 28rpx;color: #666666;}
.footer{position:fixed;z-index:20;bottom:0;display:flex;box-sizing:border-box;padding:0 32rpx;width:100%;height:100rpx;background:#fff;color:#999;font-size:24rpx;line-height:100rpx;align-items:center;justify-content:space-between}
.num .orange{font-size:38rpx}
.footer .num{display: flex;align-items: center;line-height: 100rpx;}
.footer .num text span{height: 100%;}
.footer .btn button{width:190rpx;height:74rpx;border-radius:8rpx;background:#fd711c;color:#fff;font-size:32rpx;line-height:74rpx}
.footer .btn button:after{content: normal}
